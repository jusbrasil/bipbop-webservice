    "rollup-plugin-node-resolve": "^4.2.3",
    "rollup-plugin-sourcemaps": "^0.4.2",
    "rollup-plugin-uglify": "^6.0.2",
    "core-js": "3",
    "concurrently": "^4.1.0",
    "lodash": "^4.17.11",
    "promise-limit": "^2.7.0",
